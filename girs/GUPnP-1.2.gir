        g_warning ("Failed to call GetVolume: %s", error-&gt;message);
        g_clear_error (&amp;error);

        return;
}

guint16 volume = 0;
if (!gupnp_service_proxy_action_get_result (action,
                                            &amp;error,
                                            "CurrentVolume", G_TYPE_UINT, &amp;volume,
                                            NULL)) {
        g_message ("Current Volume: %u", volume);
