{
  "compilerOptions": {   
    "lib": ["ESNext"],     
    "types": ["node"],
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "moduleResolution": "Node16"
  },
  "files": [
    "http-client.ts",
    "http-server.ts"
  ]
}
