        <parameter name="inbuf">
          <doc xml:space="preserve"
               filename="gmime-iconv.h"
               line="37">input buffer</doc>
        </parameter>
        <parameter name="inleft">
          <doc xml:space="preserve"
               filename="gmime-iconv.h"
               line="38">number of bytes left in @inbuf</doc>
