{
    "name": "@girs/gjs",
    "version": "3.0.0-beta.12",
    "description": "GJS TypeScript type definitions for Gjs",
    "type": "module",
    "module": "gjs.js",
    "main": "gjs.js",
    "typedoc": {
        "entryPoint": "./gjs.d.ts",
        "readmeFile": "./README.md",
        "displayName": "Gjs",
        "tsconfig": "./tsconfig.doc.json"
    },
    "exports": {
        "./tsconfig.alias.json": "./tsconfig.alias.json",
        "./ambient": "./ambient.d.ts",
        ".": {
            "import": {
                "types": "./gjs.d.ts",
                "default": "./gjs.js"
            },
            "require": {
                "types": "./gjs.d.cts",
                "default": "./gjs.cjs"
            }
        }
    },
    "scripts": {
        "test": "yarn test:esm && yarn test:cjs",
        "test:esm": "tsc --noEmit gjs.d.ts",
        "test:cjs": "tsc --noEmit gjs.d.cts"
    },
    "dependencies": {
        "@girs/gio-2.0": "workspace:^",
        "@girs/glib-2.0": "workspace:^",
        "@girs/gobject-2.0": "workspace:^"
    },
    "devDependencies": {
        "typescript": "^*"
    },
    "keywords": [
        "Gir",
        "TypeScript",
        "Gjs"
    ],
    "author": "ts-for-gir",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gjsify/ts-for-gir.git"
    },
    "bugs": {
        "url": "https://github.com/gjsify/ts-for-gir/issues"
    },
    "homepage": "https://github.com/gjsify/ts-for-gir#readme"
}
