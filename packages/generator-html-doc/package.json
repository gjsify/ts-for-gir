  "description": "HTML Documentation generator for ts-for-gir",
  "module": "lib/index.js",
  "main": "lib/index.js",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "yarn lint && yarn build:ts",
    "build:ts": "tsc",
    "clear": "yarn clear:build",
