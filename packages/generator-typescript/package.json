    "author": "Pascal Garber <pascal@artandcode.studio>",
    "files": [
        "lib",
        "packages.xml"
    ],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/gjsify/ts-for-gir/issues"
    },
