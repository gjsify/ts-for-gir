        gupnp_service_proxy_action_new ("GetVolume",
                                        // Parameters
                                        "InstanceID", G_TYPE_INT, 0,
                                        "Channel", G_TYPE_STRING, "Master",
                                        NULL);

GError *error = NULL;
gupnp_service_proxy_call_action (proxy, action, NULL, &amp;error);
